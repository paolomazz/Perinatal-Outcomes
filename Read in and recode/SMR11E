##Load SMR11_E##

SMR11E <- SMR11_E


##SMR11E
SMR11E$OTHER_CONDITION_1 <- as.factor(SMR11E$OTHER_CONDITION_1)
SMR11E$OTHER_CONDITION_2 <- as.factor(SMR11E$OTHER_CONDITION_2)
SMR11E$OTHER_CONDITION_3 <- as.factor(SMR11E$OTHER_CONDITION_3)
SMR11E$OTHER_CONDITION_4 <- as.factor(SMR11E$OTHER_CONDITION_4)
SMR11E$OTHER_CONDITION_5 <- as.factor(SMR11E$OTHER_CONDITION_5)
SMR11E$OTHER_CONDITION_6 <- as.factor(SMR11E$OTHER_CONDITION_6)
SMR11E$OTHER_CONDITION_7 <- as.factor(SMR11E$OTHER_CONDITION_7)
SMR11E$OTHER_CONDITION_8 <- as.factor(SMR11E$OTHER_CONDITION_8)



##relabel condition variables in smr11e to identify LBW (<2500grams) cases recorded during pregnancy
table(SMR11E$BIRTH_WEIGHT)
is.numeric(SMR11E$BIRTH_WEIGHT)
SMR11E$BIRTH_WEIGHT[SMR11E$BIRTH_WEIGHT < 500] <- NA
SMR11E <- SMR11E %>% mutate(LBW = case_when(BIRTH_WEIGHT < 2500 ~ "LBW", TRUE ~ "No_LBW"))
SMR11E$LBW <- fct_other(SMR11E$LBW, keep = 'LBW', other_level = 'No_LBW')

table(SMR11E$LBW)



