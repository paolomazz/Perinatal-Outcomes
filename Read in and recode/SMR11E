  mutate(CongenitalConditions = case_when(MAIN_CONDITION == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_1 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_2 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_3 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_4 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_5 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_6 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_7 == "Yes" ~ "CongenitalConditions",
                                          OTHER_CONDITION_8 == "Yes" ~ "CongenitalConditions",
                                          TRUE ~ "No_CongenitalConditions"))
SMR11E$CongenitalConditions <- fct_other(SMR11E$CongenitalConditions, keep = 'CongenitalConditions', other_level = 'No_CongenitalConditions')

table(SMR11E$CongenitalConditions)

##subset smr11e to include only relevant variables
