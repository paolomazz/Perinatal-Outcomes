R Script SMR11C

##Load in SMR11_C ##

SMR11C <- SMR11_C


##recode diagnosis variables to identify Congenital conditions cases in SMR11C
table(SMR11C$DIAGNOSIS_1)

SMR11C$DIAGNOSIS_1 <- as.factor(SMR11C$DIAGNOSIS_1)
SMR11C$DIAGNOSIS_2 <- as.factor(SMR11C$DIAGNOSIS_2)
SMR11C$DIAGNOSIS_3 <- as.factor(SMR11C$DIAGNOSIS_3)
SMR11C$DIAGNOSIS_4 <- as.factor(SMR11C$DIAGNOSIS_4)
SMR11C$DIAGNOSIS_5 <- as.factor(SMR11C$DIAGNOSIS_5)
SMR11C$DIAGNOSIS_6 <- as.factor(SMR11C$DIAGNOSIS_6)
SMR11C$DIAGNOSIS_7 <- as.factor(SMR11C$DIAGNOSIS_7)
SMR11C$DIAGNOSIS_8 <- as.factor(SMR11C$DIAGNOSIS_8)
SMR11C$DIAGNOSIS_9 <- as.factor(SMR11C$DIAGNOSIS_9)




##relabel condition variables in SMR02 to identify LBW (<2500grams) cases recorded during pregnancy
table(SMR11C$BIRTH_WEIGHT)
is.numeric(SMR11C$BIRTH_WEIGHT)
SMR11C$BIRTH_WEIGHT[SMR11C$BIRTH_WEIGHT < 500] <- NA
SMR11C <- SMR11C %>% mutate(LBW = case_when(BIRTH_WEIGHT < 2500 ~ "LBW", TRUE ~ "No_LBW"))
SMR11C$LBW <- fct_other(SMR11C$LBW, keep = 'LBW', other_level = 'No_LBW')

table(SMR11C$LBW)


##SMR11C congenital conditions
##Use this code for any congenital condition including chromosomal abnormalities
