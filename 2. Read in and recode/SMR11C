SMR11C <- SMR11C %>%
  mutate(CongenitalConditions = case_when(DIAGNOSIS_1 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_2 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_3 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_4 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_5 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_6 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_7 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_8 == "Yes" ~ "CongenitalConditions",
                                          DIAGNOSIS_9 == "Yes" ~ "CongenitalConditions",
                                          TRUE ~ "No_CongenitalConditions"))
SMR11C$CongenitalConditions <- fct_other(SMR11C$CongenitalConditions, keep = 'CongenitalConditions', other_level = 'No_CongenitalConditions')

table(SMR11C$CongenitalConditions)


##subset smr11c to include only relevant variables
SMR11C2 <- subset(SMR11C, select = c("child_id", "CongenitalConditions", "LBW"))
